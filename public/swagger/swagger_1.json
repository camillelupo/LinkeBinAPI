                        }
                    },
                    "403": {
                        "description": "Access denied"
                    },
                    "404": {
                        "description": "Url doesn't exist",
                        "content": {
                            "application/json": {
                                "schema": {},
                                "example": "URL Doesn't Exist"
                            }
                        }
                    }
                }
            }
        },
