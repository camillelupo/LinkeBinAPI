            "post": {
                "tags": [
                    "city"
                ],
                "summary": "Create a city",
                "operationId": "createCities",
                "parameters": [
                    {
                        "name": "Link",
                        "in": "header",
                        "description": "Link of json file",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Success",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/city"
                                },
                                "example": "\r\n     *         id: 0,\r\n     *         file_json: [ string ],\r\n     *         name: string,\r\n     *         region: string,\r\n     *         departement: string,\r\n     *         cityBins: 0\r\n     *         "
